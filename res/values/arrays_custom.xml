        <item>Daily</item>
        <item>Weekly</item>
        <item>Monthly</item>
    </string-array>

    <!--  Data usage settings. Weekdays  -->
    <string-array name="data_usage_cycle_weekdays">
        <item>Sunday</item>
        <item>Monday</item>
        <item>Tuesday</item>
        <item>Wednesday</item>
        <item>Thursday</item>
        <item>Friday</item>
        <item>Saturday</item>
