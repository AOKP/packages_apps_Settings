        <item>US</item>
        <item>CA</item>
        <item>DE</item>
        <item>GB</item>
        <item>JP</item>
        <item>AU</item>
        <item>CN</item>
        <item>KR</item>
        <item>TR</item>
        <item>SG</item>
        <item>BR</item>
